            //        console.log('Error in Operation');
            //    }
            //});






            $.ajax({
                url: "../API/MemberType",
                type: "GET",
                //data: {id:10},

            }).done(function (result) {
                var newJ = $.parseJSON(result);
                $('#ddlMemberType').empty();
                $('#ddlMemberType').append('<option value= "0">' + "Select Member Type" + '</option>');
                $.each(newJ, function (idx, obj) {
                    $('#ddlMemberType').append('<option value=' + obj.mt_id + '>' + obj.mt_name + '</option>');
