        function LoadMemberType() {
            var member = new FormData()
            member.append('mt_id', 2);
            member.append('mt_name', 'ram');
            $.ajax({
                url: '/../API/Member',
                type: 'POST',
                dataType: 'json',
                data: member,
                processData: false,
                contentType: false,// not json
                complete: function (data) {
                    var mediaId = $.parseJSON(data.responseText); //?

                },
